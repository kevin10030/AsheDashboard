<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>


    <title>@ViewData["Title"] - Sapphire2.0</title>
    <link href="~/plugins/font-awesome.min.css" type="text/css" rel="stylesheet" />
    <link href="~/css/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/plugins/fontawesome-free/css/all.css" rel="stylesheet" type="text/css" />
    <link href="~/css/right_slide_menu.css" rel="stylesheet">
    <style>
        html {
            zoom: 0.8; /* Old IE only */
            /*-moz-transform: scale(0.8);
            -webkit-transform: scale(0.8);
            transform: scale(0.8);*/
        }

        .location-label::before, .device-type-label::before{
            display: none !important;
        }
        .right_manu_area{
            height:750px;
            overflow:auto;
        }
        .location-label input, .device-type-label input, .user-label input {
            display: block !important;
            opacity: 1 !important;
            position: absolute !important;
            margin-top: -16px;
            margin-left: -20px;
        }
         .main-panel{
             margin-left:-30px!important;
         }
         .top-title{
             padding-left:30px!important;
         }
         #tblAlert td{
             text-align:center!important;
         }
    </style>

    @RenderSection("Styles", required: false)

</head>
<body style="font-family:Verdana">
    <div>
        <header id="m_header" class="m-grid__item    m-header " m-minimize-offset="200" m-minimize-mobile-offset="200">
            <div class="m-container m-container--fluid m-container--full-height">
                <div class="m-stack m-stack--ver m-stack--desktop">
                    <!-- BEGIN: Brand -->
                    <div class="m-stack__item m-brand  m-brand--skin-dark " style="background:#0C2633;">
                        <div class="m-stack m-stack--ver m-stack--general">
                            <div class="m-stack__item m-stack__item--middle m-brand__logo">
                                <a href="/home/index" class="m-brand__logo-wrapper">
                                    <img alt="" src="~/images/logo.png" />
                                </a>
                            </div>

                            <div class="m-stack__item m-stack__item--middle m-brand__tools">
                                <!-- BEGIN: Left Aside Minimize Toggle -->
                                <a href="javascript:;" id="m_aside_left_minimize_toggle" class="m-brand__icon m-brand__toggler m-brand__toggler--left m--visible-desktop-inline-block">
                                    <span></span>
                                </a>
                                <!-- END -->
                                <!-- BEGIN: Responsive Aside Left Menu Toggler -->
                                <a href="javascript:;" id="m_aside_left_offcanvas_toggle" class="m-brand__icon m-brand__toggler m-brand__toggler--left m--visible-tablet-and-mobile-inline-block">
                                    <span></span>
                                </a>
                                <!-- END -->
                                <!-- BEGIN: Responsive Header Menu Toggler -->
                                <a id="m_aside_header_menu_mobile_toggle" href="javascript:;" class="m-brand__icon m-brand__toggler m--visible-tablet-and-mobile-inline-block">
                                    <span></span>
                                </a>
                                <!-- END -->
                                <!-- BEGIN: Topbar Toggler -->
                                <a id="m_aside_header_topbar_mobile_toggle" href="javascript:;" class="m-brand__icon m--visible-tablet-and-mobile-inline-block">
                                    <i class="fa fa-ellipsis-h " style="color:#797b79;font-size: 25px;"></i>
                                </a>
                                <!-- BEGIN: Topbar Toggler -->
                            </div>

                        </div>

                    </div>
                    <!-- END: Brand -->
                    <div class="m-stack__item m-stack__item--fluid m-header-head" id="m_header_nav" style="background:#0C2633;">

                        <!-- END: Horizontal Menu -->
                        <div id="m_header_topbar" class="m-topbar  m-stack m-stack--ver m-stack--general">

                            <div class="m-stack__item m-topbar__nav-wrapper">
                                <ul class="m-topbar__nav m-nav m-nav--inline">
                                    <li class="m-nav__item m-topbar__user-profile m-topbar__user-profile--img  m-dropdown m-dropdown--medium m-dropdown--arrow m-dropdown--header-bg-fill m-dropdown--align-right m-dropdown--mobile-full-width m-dropdown--skin-light" m-dropdown-toggle="click">
                                        <a href="#" class="m-nav__link m-dropdown__toggle">
                                            <span class="m-topbar__userpic">
                                                <img src="~/images/profile_img.jpg" class="m--img-rounded m--marginless m--img-centered" alt="" />
                                            </span>
                                            <span class="m-topbar__username m--hide">
                                                Nick
                                            </span>
                                        </a>
                                    </li>
                                    <li class="m-nav__item m-topbar__quick-actions m-topbar__quick-actions--img m-dropdown m-dropdown--large m-dropdown--header-bg-fill m-dropdown--arrow m-dropdown--align-right m-dropdown--align-push m-dropdown--mobile-full-width m-dropdown--skin-light" m-dropdown-toggle="click">
                                        <a href="#" class="m-nav__link m-dropdown__toggle">
                                            <span class="m-nav__link-badge m-badge m-badge--dot m-badge--info m--hide"></span>
                                            @if (String.IsNullOrEmpty(User.Identity.Name))
                                            {
                                                <span class="m-nav__link-icon" style="width: 100px;">
                                                    Anonymous
                                                </span>
                                            }
                                            else
                                            {
                                                <span class="m-nav__link-icon">
                                                    @User.Identity.Name
                                                </span>
                                            }
                                        </a>
                                        <div class="m-dropdown__wrapper">
                                            <span class="m-dropdown__arrow m-dropdown__arrow--right m-dropdown__arrow--adjust"></span>
                                            <div class="m-dropdown__inner">
                                                <div class="m-dropdown__body">
                                                    <div class="m-dropdown__content">
                                                        <ul class="m-nav m-nav--skin-light">
                                                            <li class="m-nav__item">
                                                                <a asp-controller="Auth" asp-action="Logout" class="btn m-btn--pill    btn-secondary m-btn m-btn--custom m-btn--label-brand m-btn--bolder">
                                                                    Logout
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>

                                    <li id="m_quick_sidebar_toggle" class="m-nav__item">
                                        <a href="#" class="m-nav__link m-dropdown__toggle">
                                            <span class="m-topbar__userpic">
                                                <img src="~/images/top_right_logo.png" class="m--img-rounded m--marginless m--img-centered" alt="" />
                                            </span>
                                            <span class="m-topbar__username m--hide">
                                                Nick
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- END: Topbar -->
                    </div>
                </div>
            </div>
        </header>
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <!-- BEGIN: Left Aside -->

            <div id="m_aside_left" class="m-grid__items	m-aside-left  m-aside-left--skin-dark " style="background:#0C2633; ">
                <!-- BEGIN: Aside Menu -->
                <div id="m_ver_menu"
                     class="m-aside-menu  m-aside-menu--skin-dark m-aside-menu--submenu-skin-dark " style="    height: 897px;"
                     m-menu-vertical="1"
                     m-menu-scrollable="0" m-menu-dropdown-timeout="500">
                    <ul class="m-menu__nav  m-menu__nav--dropdown-submenu-arrow " style="padding-top:1px!important;">

                        <li class="m-menu__section" style="background:#209E93; color:white;margin-bottom: 10px;">
                            <h4 class="m-menu__section-text ass" style="color: white; font-size: 14px; font-weight: bold;">
                                Betaar3 Company Admin
                            </h4>
                            <i class="m-menu__section-icon flaticon-more-v3"></i>

                        </li>
                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-home"></i>
                                <span class="m-menu__link-text">
                                    HomePage
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/home/index" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                <span>HomePage</span>
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>

                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-user"></i>
                                <span class="m-menu__link-text">
                                    Customer
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/customers/index" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                <span>Customers</span>
                                            </span>
                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>

                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-laptop"></i>
                                <span class="m-menu__link-text">
                                    Device
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">

                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/device/index" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                Device
                                            </span>
                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-chart-bar"></i>
                                <span class="m-menu__link-text">
                                    Report
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/report/alerts" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                Alert
                                            </span>
                                        </a>
                                    </li>
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/report/parameters" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                Parameters
                                            </span>
                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>
                        @if (User.IsInRole("SuperAdmin") || User.IsInRole("Admin"))
                        {
                            <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                                <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                    <i class="m-menu__link-icon fa fa-address-card"></i>
                                    <span class="m-menu__link-text">
                                        <span>Admin</span>
                                    </span>
                                    <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                                </a>
                                <div class="m-menu__submenu ">
                                    <span class="m-menu__arrow"></span>
                                    <ul class="m-menu__subnav">
                                        <li class="m-menu__item " aria-haspopup="true">
                                            <a href="/admin/index" class="m-menu__link ">
                                                <i class="m-menu__link-bullet m-menu__link-bullet">
                                                    <span></span>
                                                </i>
                                                <span class="m-menu__link-text">
                                                    Admin
                                                </span>
                                            </a>
                                        </li>

                                        <li class="m-menu__item " aria-haspopup="true">
                                            <a href="/admin/composeMail" class="m-menu__link ">
                                                <i class="m-menu__link-bullet m-menu__link-bullet">
                                                    <span></span>
                                                </i>
                                                <span class="m-menu__link-text">
                                                    Compose Mail
                                                </span>
                                            </a>
                                        </li>

                                    </ul>
                                </div>
                                <div style="border-bottom: 1px solid #1c7563;"></div>
                            </li>
                        }

                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-map-marker-alt" style="font-size:25px;"></i>
                                <span class="m-menu__link-text">
                                    Location
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/location/index" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                Location
                                            </span>
                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>
                        <li class="m-menu__item  m-menu__item--submenu " aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon fa fa-video" style="font-size:24px;"></i>
                                <span class="m-menu__link-text">
                                    Video
                                </span>
                                <i class="m-menu__ver-arrow fa fa-angle-right" style="color:forestgreen"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/video/index" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                Video
                                            </span>
                                        </a>
                                    </li>
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="/video/SingleVideo" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text">
                                                SingleVideo
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div style="border-bottom: 1px solid #1c7563;"></div>
                        </li>
                    </ul>
                </div>
                <!-- END: Aside Menu -->
            </div>
            <!-- END: Left Aside -->
            <div class="container-fluid body-content" style="background-image: linear-gradient(to bottom, #ffffff, #f0f3fa, #dae9f4, #c2e1e8, #add8d6);height:900px;padding-right:0px">
                @RenderBody()
            </div>
        </div>
    </div>

    <partial name="_CookieConsentPartial" />

    <script src="~/plugins/jquery/dist/jquery.min.js"></script>
    <script src="~/js/vendors.bundle.js" type="text/javascript"></script>
    <script src="~/js/scripts.bundle.js" type="text/javascript"></script>
    <script src="~/plugins/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>
